        </xs:sequence>
        <xs:attributeGroup ref="identifier.ResponseDeclaration.Attr" />
        <xs:attributeGroup ref="cardinality.ResponseDeclaration.Attr" />
        <xs:attributeGroup ref="baseType.ResponseDeclaration.Attr" />
    </xs:complexType>

